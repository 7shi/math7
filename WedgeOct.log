&\quad +(a_2b_4-a_4b_2+a_5b_3-a_3b_5)\vec{e_6} \\
&\quad +(a_6b_1-a_1b_6+a_2b_5-a_5b_2+a_3b_4-a_4b_3)\vec{e_7} \\
